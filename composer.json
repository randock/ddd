{
  "name": "randock/ddd",
  "description": "Randock Ddd",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Randock\\Ddd\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Randock\\Ddd\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "7.2"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "bornfreee/tactician-domain-events": "^0.3.1",
    "symfony/validator": "^4.0|^5.0"
  },
  "require-dev": {
    "randock/build": "dev-symfony4",
    "symfony/phpunit-bridge": "^4.0|^5.0"
  }
}
